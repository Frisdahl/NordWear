{
  "name": "clothing-webshop",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=client & npm run dev --workspace=clothing-webshop-server",
    "build": "npm run build --workspaces",
    "start": "npm run start --workspace=clothing-webshop-server",
    "db:seed": "prisma db seed",
    "lint": "echo 'No linting configured'",
    "typecheck": "npm exec --workspaces -- tsc --noEmit",
    "test": "npm test --workspaces"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "prisma": "^4.16.2",
    "typescript": "^4.5.0",
    "vite": "^2.6.0"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "@react-google-maps/api": "^2.20.8",
    "@sendgrid/mail": "^8.1.6",
    "@stripe/stripe-js": "^8.6.0",
    "@types/nodemailer": "^7.0.4",
    "express": "^4.17.1",
    "mysql2": "^2.2.5",
    "nodemailer": "^7.0.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.10.1",
    "stripe": "^20.1.0",
    "swiper": "^12.0.3",
    "zustand": "^5.0.9"
  }
}
